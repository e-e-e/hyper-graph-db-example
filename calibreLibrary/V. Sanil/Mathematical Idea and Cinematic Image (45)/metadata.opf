        <dc:title>Mathematical Idea and Cinematic Image</dc:title>
        <dc:creator opf:file-as="Sanil, V., , ," opf:role="aut">V. Sanil</dc:creator>
        <dc:contributor opf:file-as="calibre" opf:role="bkp">calibre (2.47.0) [http://calibre-ebook.com]</dc:contributor>
